  "keywords": [
    "blessed",
    "curses",
    "tui",
    "tput",
    "terminfo",
    "termcap"
  ],
  "tags": [
    "curses",
    "tui",
    "tput",
    "terminfo",
    "termcap"
  ],
  "engines": {
