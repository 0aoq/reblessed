{
  "name": "reblessed",
  "description": "A high-level terminal interface library for node.js.",
  "author": "Christopher Jeffrey / kenan238",
  "version": "0.2.1",
  "license": "MIT",
  "main": "./dist/lib/reblessed.js",
  "bin": "./bin/tput.js",
  "preferGlobal": false,
